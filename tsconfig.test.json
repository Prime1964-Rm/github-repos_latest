{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "isolatedModules": true,
      "jsx": "react",
      "baseUrl": ".", 
      "paths": {
        "@/*": ["./src/*"]
      }
    }
  }
  